{
  "name": "fruitloops",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "npx webpack serve -c ./webpack.config.js",
    "test": "jest"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": "eslint --fix --cache"
  },
  "devDependencies": {
    "@swc/core": "^1.3.7",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^12.5.0",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "axios": "^1.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*{spec,test}.{js,jsx}"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "resetMocks": true
  }
}
